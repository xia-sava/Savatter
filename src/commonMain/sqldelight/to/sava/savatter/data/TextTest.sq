
CREATE TABLE textTest (
  id INTEGER NOT NULL PRIMARY KEY AUTOINCREMENT,
  text1 TEXT NOT NULL DEFAULT '',
  text2 TEXT NOT NULL DEFAULT ''
);

selectAll:
SELECT * FROM textTest ORDER BY id ASC;

deleteAll:
DELETE FROM textTest;

insert:
INSERT INTO textTest (text1, text2) VALUES (:text1, :text2);

update:
UPDATE textTest SET text1 = :text1, text2 = :text2 WHERE id = :id;

delete:
DELETE FROM textTest WHERE id = :id;

count:
SELECT COUNT(id) FROM textTest;
